{
  "schemaVersion": 1,
  "id": "grapplemod",
  "version": "${version}",
  "name": "Grappling Hook Mod",
  "description": "Fabric port of the Grappling Hook Mod",
  "authors": ["Yyon", "Nyfaria", "Mayesnake", "CG360"],
  "contact": {},
  "license": "GPL-3.0",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
      "main": ["me.cg360.mod.grapple.GrappleMod"],
      "client": ["me.cg360.mod.grapple.client.GrappleModClient"],
      "modmenu": ["me.cg360.mod.grapple.client.integration.ModMenuIntegration"]
  },
  "mixins": [
      "grapplemod.mixins.json",
      {
          "environment": "client",
          "config": "grapplemod-client.mixins.json"
      }
  ],
  "accessWidener": "grapplemod.accesswidener",
  "depends": {
      "fabricloader": ">=${loader_version}",
      "fabric": "*",
      "minecraft": "${minecraft_version_long}",
      "cloth-config": ">=${clothconfig_version}"
  },
  "recommends": {
      "modmenu": ">=${modmenu_version}"
  }
}
